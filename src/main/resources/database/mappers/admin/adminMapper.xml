<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.starbucks.sw4.admin.admin.AdminDAO">

	<select id="getAdminLogin" parameterType="AdminMemberDTO" resultType="AdminMemberDTO">
		select * from (select * from member where id=#{id} and pw=#{pw}) where type between 2 and 4
	</select>

</mapper>